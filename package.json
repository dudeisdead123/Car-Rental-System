{
  "name": "car-rental-system",
  "version": "1.0.0",
  "description": "Full-stack Car Rental System using MERN",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && npm install --prefix frontend",
    "seed": "node backend/seed.js"
  },
  "keywords": [
    "car",
    "rental",
    "mern",
    "mongodb",
    "express",
    "react",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "razorpay": "^2.9.6",
    "stripe": "^20.1.0",
    "twilio": "^5.11.1"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22"
  }
}
